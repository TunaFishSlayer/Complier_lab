program maTranTamGiacTren;
var
  n: integer;
  i: integer;
  j: integer;
  x: integer;
  isUpper: integer;
begin
  n := ReadI;
  isUpper := 1;              
  for i := 1 to n do
    for j := 1 to n do
    begin
      x := ReadI;
      if i > j then
        if x != 0 then  
          isUpper := 0;
    end;
  call WriteI(isUpper);
  call WriteLn;
end.
